		      <Size>count</Size> 
		      <ValuePointer>(TElem*)data</ValuePointer> 
		    </ArrayItems> 
  		</Expand>         
    </Type>
    <Type Name="pvect&lt;*&gt;">
        <DisplayString>{count}/{capacity}</DisplayString>
