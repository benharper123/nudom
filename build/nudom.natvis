		    <Item Name="count">count</Item> 
		    <Item Name="capacity">capacity</Item> 
		    <ArrayItems> 
		      <Size>count</Size> 
		      <ValuePointer>(TElem*)data</ValuePointer> 
		    </ArrayItems> 
  		</Expand>         
    </Type>
</AutoVisualizer>