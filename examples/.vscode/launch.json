{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Launch Example",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/../t2-output/macosx-clang-debug-default/Reactive",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [
				{
					"name": "LD_LIBRARY_PATH",
					"value": "${workspaceRoot}/../t2-output/macosx-clang-debug-default"
				}
			],
			"externalConsole": true,
			"MIMode": "lldb"
		},
		{
			"name": "(gdb) Launch Example",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/../t2-output/macosx-clang-debug-default/Reactive",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [
				{
					"name": "LD_LIBRARY_PATH",
					"value": "${workspaceRoot}/../t2-output/macosx-clang-debug-default"
				}
			],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}